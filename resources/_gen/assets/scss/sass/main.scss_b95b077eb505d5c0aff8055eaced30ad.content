*{margin:0;padding:0;box-sizing:border-box}body{background-color:#fdf7f2;font-family:sans-serif}li{list-style:none}.container{max-width:720px;margin:0 auto;padding:0 8px}header{max-width:100vw;height:4rem;background-color:#94b49f;color:#fdf7f2;border-bottom:2px dashed #576f72}header .container{display:flex;flex-direction:row;justify-content:space-between}header .container .logo{width:120px;height:4rem;display:flex;align-items:center}header .container .logo a{color:#576f72;font-weight:bolder;text-decoration:none}header .container .logo img{width:24px;height:24px;margin-left:.5rem}header .container .links{width:550px;height:4rem;display:flex}header .container .links .links-child{height:4rem;padding-left:1rem;display:flex;align-items:center}header .container .links a{color:#fdf7f2}header .container .github_icon a,header .container .github_icon{width:64px;height:4rem;display:flex;justify-content:center;align-items:center}